<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="ThemeNekoManju" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#cbae82</item>
        <item name="colorPrimaryVariant">#997f55</item>
        <item name="colorOnPrimary">#997f55</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#c0ca33</item>
        <item name="colorSecondaryVariant">#f5fd67</item>
        <item name="colorOnSecondary">#8c9900</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
